(* ===  functions in environment  =================== *)
val
   f : 'a → 'b → 'c → 'a
    | f x _ y = x

val
   g : 'a → 'b → 'c → 'b
    | g _ x _ = x

val
   h : 'a → 'b → 'c → 'd → unit
    | h _ _ _ _ = ()

(* ================================================== *)

(* ===  functions in environment  =================== *)
val
   f : 'a → 'b → 'c → 'a
    | f x _₁ y = x

val
   g : 'a → 'b → 'c → 'b
    | g _₁ x _₂ = x

val
   h : 'a → 'b → 'c → 'd → unit
    | h _₁ _₂ _₃ _₄ = ()

(* ================================================== *)

